{
  "name": "roverrobotics.com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "shopify:upload:assets": "$(npm bin)/shopify-themekit --config shopify.yml upload assets",
    "shopify:watch": "theme watch",
    "copy:static:shop": "find ./static -type f -exec cp {} ./shop/assets/ \\;",
    "copy:static:blog": "cp -R ./static/ ./blog/wp-content/themes/roverrobotics/assets/",
    "copy:static:docs": "cp -R ./static/ ./docs/assets/",
    "shop:developer": "NODE_ENV='development' concurrently \"webpack --config webpack.shop.config.js\" \"npm run shopify:upload:assets\" \"npm run shopify:watch\"",
    "shop:dev": "npm run copy:static:shop && npm run shop:developer",
    "shop:builder": "webpack --config webpack.shop.config.js --watch",
    "shop:build": "npm run copy:static:shop && npm run shop:builder",
    "blog:developer": "NODE_ENV='development' webpack --config webpack.blog.config.js",
    "blog:dev": "npm run copy:static:blog && npm run blog:developer",
    "blog:builder": "NODE_ENV='production' webpack --config webpack.blog.config.js",
    "blog:build": "npm run copy:static:blog && npm run blog:builder",
    "docs:serve": "cd docs && bundle exec jekyll serve",
    "docs:developer": "NODE_ENV='development' concurrently \"webpack --config webpack.docs.config.js\" \"npm run docs:serve\"",
    "docs:dev": "npm run copy:static:docs && npm run docs:developer",
    "docs:builder": "NODE_ENV='production' webpack --config webpack.docs.config.js",
    "docs:build": "npm run copy:static:docs && npm run docs:builder",
    "forums:builder": "NODE_ENV='production' webpack --config webpack.forums.config.js",
    "forums:build": "npm run forums:builder",
    "dev": "concurrently \"npm run blog:dev\" \"npm run shop:dev\" \"npm run docs:dev\"",
    "build": "npm run shop:build && npm run blog:build && npm run docs:build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@shopify/themekit": "^0.6.12",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "json-loader": "^0.5.7",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "postcss-scss": "^1.0.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "fitvids": "^2.0.0",
    "gsap": "^1.20.3",
    "jquery": "^3.2.1",
    "jquery-pjax": "^2.0.1",
    "jquery-ui": "^1.12.1",
    "lazysizes": "^4.0.4",
    "load-google-maps-api": "^1.3.2",
    "lodash": "^4.17.5",
    "masonry-layout": "^4.2.1",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "objectFitPolyfill": "^2.1.0",
    "qs": "^6.5.2",
    "raf": "^3.4.0",
    "rellax": "^1.6.2",
    "scrollreveal": "^3.4.0",
    "slick-carousel": "^1.8.1"
  }
}
